{
  "name": "avviewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "jest && vue-cli-service build --dest ../public/dist",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "webserve": "NODE_ENV=website vue-cli-service serve",
    "webuild": "NODE_ENV=website vue-cli-service build --dest website"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@vue/test-utils": "^1.3.6",
    "axios": "^0.26.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.39.0",
    "document-register-element": "^1.14.10",
    "fibers": "^4.0.3",
    "flush-promises": "^1.0.2",
    "iso8601-duration": "^1.3.0",
    "lodash": "^4.17.21",
    "lunr": "^2.3.8",
    "node-notifier": "8.0.1",
    "node-webvtt": "^1.9.4",
    "sass": "^1.82.0",
    "sass-loader": "^8.0.2",
    "video.js": "^7.21.6",
    "videojs-offset": "^2.1.3",
    "videojs-seek-buttons": "^1.6.0",
    "videojs-vtt-thumbnails": "0.0.13",
    "videojs-wavesurfer": "^3.10.0",
    "vue": "^2.7.16",
    "vue-custom-element": "^3.3.0",
    "vue-js-modal": "^2.0.1",
    "vue-router": "^3.6.5",
    "webpack": "^4.47.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.5.2",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.7.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "setupFiles": [
      "core-js",
      "jest-canvas-mock"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "WaveSurfer": "<rootDir>/node_modules/wavesurfer.js",
      "wavesurfer": "<rootDir>/node_modules/videojs-wavesurfer",
      "seekButtons": "<rootDir>/node_modules/videojs-seek-buttons"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
